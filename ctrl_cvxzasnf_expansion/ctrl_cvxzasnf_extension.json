{
    "title": "Change control to command keys v4",
    "rules": [
        {
            "description": "Change left_control + CXVZSAWFN to left_command + CXVZSAWFN ver 4",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "c",
                        "modifiers": {
                            "mandatory": [
                                "left_control"
                            ],
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "c",
                            "modifiers": [
                                "left_command"
                            ]
                        }
                    ],
                    "conditions": [
                        {
                            "type": "frontmost_application_unless",
                            "bundle_identifiers": [
                                "^com\\.parallels\\.desktop\\.console",
                                "^com\\.vmware\\.horizon",
                                "^com\\.apple\\.Terminal",
                                "^com\\.googlecode\\.iterm2"
                            ]
                        }
                    ]
